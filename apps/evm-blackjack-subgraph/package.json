{
  "name": "EVMBlackjack",
  "version": "0.1.0",
  "scripts": {
    "build-contract": "solc contracts/Gravity.sol --abi -o abis --overwrite && solc contracts/Gravity.sol --bin -o bin --overwrite",
    "create": "graph create nickadamson/EVMBlackjack --node https://api.thegraph.com/deploy/",
    "create-local": "graph create nickadamson/EVMBlackjack --node http://127.0.0.1:8020",
