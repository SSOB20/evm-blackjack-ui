{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev --no-cache  --continue",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test"
  },
  "devDependencies": {
    "prettier": "^2.8.4",
    "turbo": "latest"
  },
  "packageManager": "pnpm@7.15.0",
  "pnpm": {
    "overrides": {
      "decode-uri-component@<0.2.1": ">=0.2.1",
      "json5@<1.0.2": ">=1.0.2"
    }
  }
}
